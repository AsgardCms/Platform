{
  "name": "idavoll/translation-module",
  "description": "Module containing all AsgardCms translations",
  "type": "asgard-module",
  "license": "MIT",
  "require": {
    "php": "^8.0",
    "composer/installers": "~1.0",
    "league/csv": "~9.8.0",
    "venturecraft/revisionable": "^1.39"
  },
  "require-dev": {
    "phpunit/phpunit": "~9.5",
    "orchestra/testbench": "~v7.16"
  },
  "autoload": {
    "psr-4": {
      "Modules\\Translation\\": ""
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Modules\\Translation\\": ".",
      "Modules\\": "Modules/"
    }
  },
  "authors": [
    {
      "name": "Nicolas Widart",
      "email": "n.widart@gmail.com"
    }
  ],
  "extra": {
    "branch-alias": {
      "dev-master": "4.0.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "composer/installers": false
    }
  }
}
