{
  "name": "idavoll/workshop-module",
  "type": "asgard-module",
  "description": "Workshop module for AsgardCMS. Handles the activation of modules.",
  "keywords": [
    "asgardcms",
    "workshop",
    "modules"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Nicolas Widart",
      "email": "info@asgardcms.com",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "support@asgardcms.com",
    "issues": "https://github.com/AsgardCms/Platform/issues",
    "source": "https://github.com/AsgardCms/Workshop"
  },
  "require": {
    "php": "^8.0",
    "composer/installers": "~1.0",
    "symfony/yaml": "^v6.2.2"
  },
  "require-dev": {
    "phpunit/phpunit": "~9.5",
    "orchestra/testbench": "^7.16",
    "spatie/phpunit-snapshot-assertions": "^4.2.16"
  },
  "autoload-dev": {
    "psr-4": {
      "Modules\\Workshop\\": ".",
      "Modules\\": "Modules/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "4.0.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "composer/installers": false
    }
  }
}
